   "source": [
    "import zhtts\n",
    "\n",
    "text = \"2020年，这是一个开源的端到端中文语音合成系统\"\n",
    "tts = zhtts.TTS() # use fastspeech2 by default\n",
    "\n",
    "tts.text2wav(text, \"test_data/demo.wav\")\n",
    "tts.frontend(text)\n",
    "tts.synthesis(text)"
   ]
